{
	"name": "eurynome-cloud-ui",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"serve": "vite preview",
		"build": "vite build --mode production",
		"dev": "cross-env vite --force --host --mode development",
		"prod": "cross-env vite --force --host --mode production",
		"report": "rimraf dist && cross-env vite build --mode production",
		"preview": "vite preview",
		"push": "yarn commit && git push --set-upstream origin quasar",
		"commit": "git add . && git status && git-cz"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky-run": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.(js|json|md|scss|ts|vue)": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@bpmn-io/properties-panel": "^0.14.0",
		"@quasar/extras": "^1.14.1",
		"@vuelidate/core": "^2.0.0-alpha.42",
		"@vuelidate/validators": "^2.0.0-alpha.30",
		"animate.css": "^4.1.1",
		"axios": "^0.27.2",
		"bpmn-js": "^9.2.2",
		"bpmn-js-properties-panel": "^1.1.1",
		"bpmn-js-token-simulation": "^0.27.0",
		"camunda-bpmn-js-behaviors": "^0.1.0",
		"camunda-bpmn-moddle": "^6.1.2",
		"colord": "^2.9.2",
		"diagram-js": "^8.5.0",
		"echarts": "^5.3.3",
		"js-base64": "^3.7.2",
		"lodash-es": "^4.17.21",
		"moment": "^2.29.3",
		"particles.vue3": "^2.1.0",
		"pinia": "^2.0.14",
		"pinia-plugin-persistedstate": "^1.6.1",
		"qs": "^6.11.0",
		"quasar": "^2.7.3",
		"sm-crypto": "^0.3.8",
		"sweetalert2": "^11.4.19",
		"tsparticles": "^2.1.0",
		"vue": "^3.2.37",
		"vue-i18n": "^9.1.10",
		"vue-router": "^4.0.16"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "^1.0.10",
		"@types/bpmn-moddle": "^5.1.6",
		"@types/lodash-es": "^4.17.6",
		"@types/qs": "^6.9.7",
		"@types/sm-crypto": "^0.3.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vitejs/plugin-vue": "^2.3.3",
		"commitizen": "^4.2.4",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.1",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^9.1.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"postcss": "^8.4.14",
		"postcss-import": "^14.1.0",
		"postcss-nesting": "^10.1.9",
		"postcss-preset-env": "^7.7.2",
		"preact": "^10.8.2",
		"prettier": "^2.7.1",
		"rollup": "^2.75.7",
		"rollup-plugin-visualizer": "^5.6.0",
		"sass": "^1.53.0",
		"typescript": "^4.7.4",
		"unplugin-icons": "^0.14.6",
		"unplugin-vue-components": "^0.20.1",
		"vite": "^2.9.13",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vue-tsc": "^0.38.2",
		"webpack": "^5.73.0",
		"zeebe-bpmn-moddle": "^0.12.1"
	}
}
