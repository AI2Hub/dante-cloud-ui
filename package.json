		"commitizen": "^4.2.5",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^9.6.0",
