{
  "name": "dante-cloud-ui-next",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "git add . && git status && git-cz",
    "push": "pnpm run commit && git push --set-upstream origin monorepo",
    "dev": "pnpm run --filter=@dante/ui dev",
    "shared:build": "pnpm -r --filter=@herodotus/* run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky-run": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|json|md|scss|ts|vue)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.3.0",
    "qs": "^6.11.0",
    "quasar": "^2.10.0",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "@quasar/vite-plugin": "^1.2.3",
    "@types/lodash-es": "^4.17.6",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-quasar": "^1.1.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "rollup": "2.79.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "terser": "^5.15.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^1.0.9"
  }
}
