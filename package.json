{
	"name": "herodotus-cloud-ui",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"serve": "vite preview",
		"build": "vite build --mode production",
		"dev": "vite --force --host --mode development",
		"preview": "vite preview",
		"push": "yarn commit && git push --set-upstream origin vuetify3",
		"commit": "git add . && git status && git-cz"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky-run": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.(js|json|md|scss|ts|vue)": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@mdi/font": "6.6.96",
		"json-stringify-safe": "^5.0.1",
		"localforage": "^1.10.0",
		"lodash-es": "^4.17.21",
		"moment": "^2.29.3",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.13",
		"pinia-plugin-persistedstate-2": "^1.0.0",
		"roboto-fontface": "*",
		"vue": "^3.2.25",
		"vue-i18n": "^9.1.9",
		"vue-router": "^4.0.14",
		"vuetify": "^3.0.0-beta.0",
		"webfontloader": "^1.0.0"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.6",
		"@types/webfontloader": "^1.0.0",
		"@vitejs/plugin-vue": "^2.3.1",
		"@vue/compiler-sfc": "^3.2.33",
		"@vuetify/vite-plugin": "^1.0.0-alpha.0",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.13.0",
		"eslint-plugin-vue": "^8.6.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.8",
		"prettier": "^2.6.2",
		"sass": "^1.38.0",
		"sass-loader": "^12.6.0",
		"typescript": "^4.5.4",
		"vite": "^2.9.5",
		"vue-cli-plugin-vuetify": "~2.4.8",
		"vue-tsc": "^0.34.7",
		"vuetify-loader": "^2.0.0-alpha.0",
		"webpack": "^5.72.0"
	}
}
