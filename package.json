    },
    "dependencies": {
        "@babel/runtime": "^7.16.7",
        "caniuse-lite": "^1.0.30001299",
        "core-js": "^3.20.3",
        "ts-vue-plugin": "^0.1.3",
        "typescript-ioc": "^3.2.2",
        "vue": "^2.6.14",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        "vuetify": "^2.6.2"
    },
    "peerDependencies": {
        "vue": "^2.6.14",
        "vuetify": "^2.6.1"
    }
}
