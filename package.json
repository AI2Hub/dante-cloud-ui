{
	"name": "herodotus-cloud-ui",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"serve": "vite preview",
		"build": "vite build --mode production",
		"dev": "vite --force --host --mode development",
		"preview": "vite preview",
		"push": "yarn commit && git push --set-upstream origin vuetify3",
		"commit": "git add . && git status && git-cz"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky-run": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.(js|json|md|scss|ts|vue)": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@mdi/font": "6.6.96",
		"@vee-validate/i18n": "^4.5.11",
		"@vee-validate/rules": "^4.5.11",
		"animate.css": "^4.1.1",
		"axios": "^0.27.2",
		"colord": "^2.9.2",
		"js-base64": "^3.7.2",
		"localforage": "^1.10.0",
		"lodash-es": "^4.17.21",
		"moment": "^2.29.3",
		"nprogress": "^0.2.0",
		"particles.vue3": "^2.0.6",
		"pinia": "^2.0.14",
		"pinia-plugin-persistedstate": "^1.5.1",
		"qs": "^6.10.3",
		"roboto-fontface": "*",
		"sm-crypto": "^0.3.8",
		"sweetalert2": "^11.4.11",
		"tsparticles": "^2.0.6",
		"vee-validate": "^4.5.11",
		"vue": "^3.2.25",
		"vue-i18n": "^9.1.10",
		"vue-router": "^4.0.15",
		"vuetify": "^3.0.0-beta.1",
		"webfontloader": "^1.0.0"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.6",
		"@types/qs": "^6.9.7",
		"@types/sm-crypto": "^0.3.0",
		"@types/webfontloader": "^1.0.0",
		"@vitejs/plugin-vue": "^2.3.2",
		"@vue/compiler-sfc": "^3.2.33",
		"@vuetify/vite-plugin": "^1.0.0-alpha.0",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.15.0",
		"eslint-plugin-vue": "^8.7.1",
		"husky": "^7.0.4",
		"lint-staged": "^12.4.1",
		"prettier": "^2.6.2",
		"sass": "^1.51.0",
		"sass-loader": "^12.6.0",
		"typescript": "^4.6.4",
		"vite": "^2.9.8",
		"vue-cli-plugin-vuetify": "~2.4.8",
		"vue-tsc": "^0.34.11",
		"vuetify-loader": "^2.0.0-alpha.0",
		"webpack": "^5.72.0"
	}
}
