		"bpmn-js-properties-panel": "1.9.0",
		"bpmn-js-token-simulation": "^0.28.0",
		"camunda-bpmn-js-behaviors": "^0.2.2",
		"camunda-bpmn-moddle": "^7.0.1",
		"colord": "^2.9.3",
		"diagram-js": "^9.1.0",
		"echarts": "^5.4.0",
		"js-base64": "^3.7.2",
		"lodash-es": "^4.17.21",
		"moment": "^2.29.4",
		"particles.vue3": "^2.3.3",
		"pinia": "^2.0.23",
		"pinia-plugin-persistedstate": "^2.3.0",
		"qs": "^6.11.0",
		"quasar": "^2.9.2",
		"simple-uploader.js": "^0.6.0",
		"sm-crypto": "^0.3.11",
		"spark-md5": "^3.0.2",
		"sweetalert2": "^11.4.38",
		"tsparticles": "^2.3.4",
		"vue": "^3.2.40",
		"vue-i18n": "^9.2.2",
		"vue-router": "4.1.5",
